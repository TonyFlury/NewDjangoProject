<?xml version="1.0" encoding="UTF-8"?>
<module type="PYTHON_MODULE" version="4">
  <component name="NewModuleRootManager">
    <content url="file://$MODULE_DIR$">
      <sourceFolder url="file://$MODULE_DIR$/{{cookiecutter.project_repo}}" isTestSource="false" />
      <sourceFolder url="file://$MODULE_DIR$/tests" isTestSource="true" />
      <sourceFolder url="file://$MODULE_DIR$/docs" isTestSource="false" />
      <excludeFolder url="file://$MODULE_DIR$/.eggs" />
      <excludeFolder url="file://$MODULE_DIR$/build" />
      <excludeFolder url="file://$MODULE_DIR$/dist" />
      <excludeFolder url="file://$MODULE_DIR$/docs/_build" />
      <excludeFolder url="file://$MODULE_DIR$/docs/_build_html" />
      <excludeFolder url="file://$MODULE_DIR$/docs/_static" />
      <excludeFolder url="file://$MODULE_DIR$/docs/_templates" />
      <excludeFolder url="file://$MODULE_DIR$/htmlcov" />
      <excludeFolder url="file://$MODULE_DIR$/{{cookiecutter.project_repo}}.egg-info" />
    </content>
    {%- if cookiecutter.Py35 == "Yes" -%}
    <orderEntry type="jdk" jdkName="Python 3.5.2 virtualenv at ~/.envs/{{cookiecutter.project_repo}}35" jdkType="Python SDK" />
    {%- elif cookiecutter.Py27 == "Yes" -%}
    <orderEntry type="jdk" jdkName="Python 2.7.12 virtualenv at ~/.envs/{{cookiecutter.project_repo}}27" jdkType="Python SDK" />
    {%- endif -%}
    <orderEntry type="sourceFolder" forTests="false" />
  </component>
</module>
